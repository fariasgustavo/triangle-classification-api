{
  "name": "triangle-classification-api",
  "version": "1.0.0",
  "description": "An API to determine if a triangle is equilateral, isosceles, or scalene.",
  "main": "./src/index.ts",
  "scripts": {
    "start:dev": "npx sls offline start --stage local --host 0.0.0.0 --useChildProcesses",
    "build": "npx rimraf build && npx tsc",
    "test": "npx jest",
    "test:coverage": "npx jest --coverage",
    "deploy:dev": "npx serverless deploy --stage dev",
    "deploy:prd": "npx serverless deploy --stage prd"
  },
  "keywords": [
    "typescript",
    "nodejs",
    "api",
    "rest",
    "serverless"
  ],
  "author": "Gustavo Farias",
  "license": "ISC",
  "dependencies": {
    "aws-lambda": "^1.0.7",
    "dotenv": "^10.0.0",
    "dynamoose": "^2.8.3",
    "jsonwebtoken": "^8.5.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.89",
    "@types/jest": "^27.0.3",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/node": "16",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "aws-cdk": "2.5.0",
    "aws-sdk": "^2.1053.0",
    "aws-sdk-mock": "^5.5.1",
    "dotenv-cli": "^4.1.1",
    "eslint": "^8.5.0",
    "jest": "^27.4.5",
    "nock": "^13.2.1",
    "prettier": "^2.5.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.2",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.4",
    "serverless": "^2.49.0",
    "serverless-offline": "^6.9.0",
    "serverless-plugin-typescript": "^1.2.0"
  }
}
